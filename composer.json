{
    "name": "larsnieuwenhuizen/e-utilities",
    "description": "Php library for communication with the NCBI E-Utilities api",
    "type": "library",
    "require": {
        "php": ">=7.1.3",
        "guzzlehttp/guzzle": "^6.3",
        "vlucas/phpdotenv": "^2.5@dev"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.2",
        "phpunit/phpunit": "^7.1",
        "codeclimate/php-test-reporter": "^0.4.4"
    },
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Lars Nieuwenhuizen",
            "email": "l.nieuwenhuizen@developers.nl"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "LarsNieuwenhuizen\\EUtilities\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LarsNieuwenhuizen\\EUtilities\\Tests\\": "tests/",
            "LarsNieuwenhuizen\\EUtilities\\Commands\\": "development/Commands/",
            "LarsNieuwenhuizen\\EUtilities\\Services\\": "development/Services/"
        }
    }
}
